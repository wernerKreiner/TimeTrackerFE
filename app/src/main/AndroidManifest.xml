<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="at.jku.se.timetrackerfrontend">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true">
        <activity
            android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".ManualEntryActivity" />
        <activity android:name=".AutoEntryActivity"/>
        <activity android:name=".EditEntryDetailActivity"/>
        <activity android:name=".EditEntryActivity"/>
        <activity android:name=".CreateProjectActivity"/>
        <activity android:name=".ManageProjectActivity"/>
        <activity android:name=".ManageCategoryActivity"/>
        <activity android:name=".ManageProjectTeamActivity"/>
        <activity android:name=".ProjectReportActivity" />
        <activity android:name=".UserReportActivity" />
        <activity android:name=".SettingsActivity"/>
        <activity android:name=".MemberAddActivity" />
    </application>

</manifest>